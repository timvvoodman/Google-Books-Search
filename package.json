{
  "name": "google-books-search",
  "version": "1.0.0",
  "description": "react google books api interface",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "mongoose": "^5.11.18",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.3",
    "web-vitals": "^1.1.0",
    "if-env": "^1.0.4"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.4"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timvvoodman/Google-Books-Search.git"
  },
  "author": "Tim Woodman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/timvvoodman/Google-Books-Search/issues"
  },
  "homepage": "https://github.com/timvvoodman/Google-Books-Search#readme"
}
